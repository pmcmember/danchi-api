{
  "name": "danchi-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "jest --config=./jest.config.js",
    "test:clear": "jest --config=./jest.config.js --clearCache",
    "deploy:com": "serverless deploy --config ./infra/stacks/common-stack/serverless.yml",
    "deploy:app": "serverless deploy --config ./infra/stacks/application-stack/serverless.yml",
    "deploy:mnt": "serverless deploy --config ./infra/stacks/monitor-stack/serverless.yml",
    "remove:com": "serverless remove --config ./infra/stacks/common-stack/serverless.yml",
    "remove:app": "serverless remove --config ./infra/stacks/application-stack/serverless.yml",
    "remove:mnt": "serverless remove --config ./infra/stacks/monitor-stack/serverless.yml",
    "doc:start": "ts-node bin/openapiUtil.ts start ./docker-compose.openapi.yml",
    "doc:open": "ts-node bin/openapiUtil.ts open ./docker-compose.openapi.yml",
    "doc:stop": "ts-node bin/openapiUtil.ts stop ./docker-compose.openapi.yml",
    "doc:quit": "ts-node bin/openapiUtil.ts quit ./docker-compose.openapi.yml",
    "doc:restart": "npm run doc:stop && npm run doc:start",
    "readme:link": "doctoc README.md --github"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.17",
    "@types/jest": "^27.0.2",
    "@types/node": "^10.12.18",
    "@types/serverless": "^1.78.35",
    "aws-sdk": "^2.817.0",
    "fork-ts-checker-webpack-plugin": "^6.3.4",
    "jest": "^27.2.1",
    "microcms-js-sdk": "^2.0.0",
    "serverless": "^2.60.0",
    "serverless-webpack": "^5.5.4",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "ts-node": "^8.10.2",
    "typescript": "^3.2.4",
    "webpack": "^5.55.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@types/jsdom": "^16.2.14",
    "axios": "^0.26.1",
    "jsdom": "^19.0.0",
    "node-fetch": "^3.2.3"
  }
}
